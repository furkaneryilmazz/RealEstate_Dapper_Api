@model List<RealEstate_Dapper_Api.UI.ProductImageDtos.GetProductImageByProductIdDto>

<div class="single-post-image mb-5">
    <div class="owl-blog owl-carousel owl-theme">
        @foreach (var item in Model)
        {
            <div class="item">
                <div class="card" style="height:500px">
                    <img src="@item.ImageUrl" class="img-fluid radius-image" alt="image">
                </div>
            </div>
        }
    </div>
</div>